<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels1="clr-namespace:OneWare.Gowin.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450" IsVisible="{Binding IsVisible}"
             x:Class="OneWare.Gowin.Views.GowinLoaderWindowExtensionView" x:DataType="viewModels1:GowinLoaderWindowExtensionViewModel">
    <MenuItem Header="Gowin Loader Settings" 
              Command="{Binding OpenSettingsAsync}"
              IsEnabled="{Binding IsEnabled}">
        <MenuItem.Icon>
            <Image Source="{DynamicResource Material.SettingsOutline}"/>
        </MenuItem.Icon>
    </MenuItem>
</UserControl>
